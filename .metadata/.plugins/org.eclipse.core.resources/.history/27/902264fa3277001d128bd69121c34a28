package exceptionsHandling;

public class Exa1 {

	public static void main(String[] args) 
	{
		
		//Run Time Exception
		
		int a=10;
		int b=0;
		
		int c=a/b;
		
		System.out.println(c);  //Risky code
		
		System.out.println("Hi Ashok");
		
		//How To Handel it
		
		try 
		{
			System.out.println(c);  //Risky code
		} 
		catch (ArithmeticException e) 
		{
			
		}
		
		
		
		
	}

}
