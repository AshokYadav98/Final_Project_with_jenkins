package exceptionsHandling;

public class Exa2 {

	public static void main(String[] args) 
	{
		
		String a =null;
		
		String b="Ashok";
		
		
		
		try 
		{
			System.out.println(a.length());  //risky code
		} 
		catch (NullPointerException e) 
		{
			System.out.println("we cant calculate length of null value");
		}
		
	}

}
